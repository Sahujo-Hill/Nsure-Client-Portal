@page "/myHome"

<link rel="stylesheet" href="/css/Global.css" />

<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Readex+Pro:wght@160..700&display=swap" rel="stylesheet">

<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Inter:wght@100..900&display=swap" rel="stylesheet">

<div class="frame"> <!--Generic Frame used for all pages: FrameSTART -->


    <!--Nsure Client Portal logo-->
    <div style="margin-bottom: 35px" class="companyLogo">

        <div class="logoText">
            <span class="companyLogoN">N</span>
            <span class="companyLogoSure">sure</span>
        </div>

        <div style="width: 100%; height: 30%; background: #D9D9D9">
            <p class="companyLogoCP">
                Client Portal
            </p>
        </div>
    </div>

    <!--Policy Summary-->
    <div class="policyContainer">

        <div class="mySummary">
            <img class="myHomeGif" src="/images/home.gif" alt="House" />

            <div class="policySelectWhiteBox">
                <span class="readex40">OX1 5RX</span>
            </div>
        </div>

        <div class="policyContainerWhiteBox">

            <table class="dateTable">
                <tr>
                    <th class="inter14Green">Start date</th>
                    <td class="inter14">22 September 2023</td>
                </tr>
                <tr>
                    <th class="inter14Green">Renewal date</th>
                    <td class="inter14">22 September 2024</td>
                </tr>
            </table>

            <table class="summaryTable">
                <tr class="topRow">
                    <th class="inter14Green">Cover Type</th>
                    <td class="inter14">Contents</td>

                    <th class="inter14Green">Sum Insured</th>
                    <td class="inter14">£100,000</td>

                    <th class="inter14Green">Excess</th>
                    <td class="inter14">£5,000</td>
                </tr>
                <tr class="bottomRow">
                    <th class="inter14Green">Tier</th>
                    <td class="inter14">Platinum Cover</td>

                    <th class="inter14Green">Address</th>
                    <td class="inter14" colspan="3">23 Links Road, Kennington, Oxford</td>
                </tr>
            </table>

            <div class="policyButtonSection">

                <NavLink style="text-decoration: none" href="EMPTY" Match="NavLinkMatch.All">
                    <img src="/images/docs.png" class="docsIcon" />
                    <span class="inter14Green">View Documents</span>
                </NavLink>

                <NavLink style="text-decoration: none" href="EMPTY" Match="NavLinkMatch.All">
                    <img src="/images/edit.png" class="editIcon" />
                    <span class="inter14Green">Make a Change</span>
                </NavLink>

                <NavLink style="text-decoration: none" href="EMPTY" Match="NavLinkMatch.All">
                    <img src="/images/payments.png" class="paymentIcon" />
                    <span class="inter14Green">My Payments</span>
                </NavLink>

            </div>

        </div>

    </div>


</div> <!--Generic Frame used for all pages: FrameEND -->

@code {

}
